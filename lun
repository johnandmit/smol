#include <stdio.h>
#include<math.h>

int main(void) 
{
  int n,b;
  printf("nhap so nguyen duong: ");
  scanf("%i",&n);
  b=sqrt(n);
  b=floor(b);
  for(;b>1;b--){
    if(n%b==0){printf("n knong phai la so nguyen to");
    break;}
    else{
      printf("n la so nguyen to");
      break;}
    }
  }
